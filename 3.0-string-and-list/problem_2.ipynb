   ],
   "source": [
    "inp=input()\n",
    "numbers=inp.split()\n",
    "x=float(numbers[0])\n",
    "min_v=float(numbers[1])\n",
    "max_v=float(numbers[2])\n",
    "norm=(x-min_v)/(max_v-min_v)\n",
    "print(f\"{norm:.2f}\")"
   ]
